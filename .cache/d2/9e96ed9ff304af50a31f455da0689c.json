splay: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 1rem;\n}\n\n.hero .hero__content__btn {\n  background-color: #F16718;\n  color: #ffffff;\n}\n\n.hero .hero__content__span {\n  display: inline-block;\n  font-weight: 600;\n  color: #7b8bad;\n}\n\n.hero .hero__img__img {\n  width: 100%;\n  margin-left: 24px;\n  margin-top: 67px;\n  border-radius: 24px 0 0 24px;\n}\n\n@media screen and (min-width: 680px) {\n  .hero {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    margin-top: 65px;\n  }\n}\n\n@media screen and (min-width: 1280px) {\n  .hero {\n    padding: 0 165px;\n    gap: 140px;\n  }\n  .hero .hero__content {\n    padding: 0;\n    align-self: center;\n  }\n  .hero .hero__content__title {\n    font-size: 3.5rem;\n    line-height: 56px;\n  }\n  .hero .hero__content__description {\n    font-size: 1.125rem;\n  }\n  .hero .hero__content__primary {\n    gap: 60px;\n    justify-content: flex-start;\n  }\n}\n\n.primary {\n  display: grid;\n  grid-template-columns: 1fr 1fr 1fr;\n  margin-top: 15px;\n  gap: 70px 20px;\n}\n\n.primary .primary__img1__img,\n.primary .primary__img2__img {\n  width: 100%;\n  height: 100%;\n  border-radius: 20px;\n}\n\n.primary .primary__img1__img {\n  opacity: 0.2;\n}\n\n.primary .primary__img1 {\n  background-color: #F16718;\n  border-radius: 0 20px 20px 0;\n}\n\n.primary .primary__img2 {\n  grid-area: 1/2/1/4;\n  border-radius: 20px;\n  margin-right: 24px;\n}\n\n.primary .primary__content {\n  grid-area: 2/1/2/4;\n  margin: 0 24px;\n}\n\n.primary .primary__content__title {\n  font-size: 2rem;\n  padding: 0px 36px;\n  line-height: 36px;\n  font-weight: 900;\n  text-align: center;\n  color: #162542;\n}\n\n.primary .primary__content__description {\n  font-size: 1rem;\n  line-height: 26px;\n  color: #7b8bad;\n  margin-top: 35px;\n  margin-top: 25px;\n  text-align: center;\n}\n\n@media screen and (min-width: 680px) {\n  .primary {\n    margin-top: 48px;\n  }\n  .primary .primary__img1 {\n    border-radius: 20px;\n    margin-left: 39px;\n  }\n  .primary .primary__img2 {\n    margin-right: 39px;\n  }\n  .primary .primary__content {\n    display: flex;\n    flex-wrap: nowrap;\n    gap: 100px;\n  }\n  .primary .primary__content__title {\n    font-size: 2rem;\n    padding: 0px 15px;\n    line-height: 36px;\n    font-weight: 900;\n    text-align: left;\n    color: #162542;\n  }\n  .primary .primary__content__description {\n    text-align: left;\n    margin-right: 15px;\n  }\n}\n\n@media screen and (min-width: 1280px) {\n  .primary {\n    grid-template-columns: 1fr 2fr 1fr;\n    margin: 48px 145px 48px 165px;\n  }\n  .primary .primary__img1 {\n    margin-left: 0;\n  }\n  .primary .primary__img2 {\n    grid-area: 1/2/1/3;\n  }\n  .primary .primary__content {\n    grid-area: 1/3/1/4;\n    gap: 10px;\n    flex-direction: column;\n    justify-content: center;\n  }\n  .primary .primary__content__title {\n    padding: 0px 15px 0 0;\n  }\n  .primary .primary__content__description {\n    margin-right: 0px;\n  }\n}\n\n.secondary {\n  margin-top: 117px;\n}\n\n.secondary .cards {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.secondary .card {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  gap: 45px;\n  margin-bottom: 45px;\n}\n\n.secondary .card__image__holder {\n  background-color: #F16718;\n  width: 65px;\n  height: 65px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border-radius: 20px;\n}\n\n.secondary .card__title {\n  font-size: 1.5rem;\n  line-height: 28px;\n  font-weight: 900;\n  color: #162542;\n  margin: 0 100px;\n  text-align: center;\n}\n\n.secondary .card__description {\n  font-size: 1rem;\n  line-height: 26px;\n  color: #7b8bad;\n  margin-top: 35px;\n  margin-top: 0;\n  padding: 0 24px;\n  text-align: center;\n}\n\n@media screen and (min-width: 680px) {\n  .secondary {\n    margin-left: 39px;\n    margin-right: 98px;\n    margin-top: 134px;\n  }\n  .secondary .card {\n    align-items: baseline;\n  }\n  .secondary .card__title {\n    margin: 0px;\n    text-align: left;\n  }\n  .secondary .card__description {\n    padding: 0;\n    text-align: left;\n  }\n}\n\n@media screen and (min-width: 1280px) {\n  .secondary {\n    margin: 134px 165px 0;\n  }\n}\n\n.footer__content {\n  font-size: 1rem;\n  line-height: 26px;\n  color: #7b8bad;\n  margin-top: 35px;\n  text-align: center;\n  padding-bottom: 24px;\n}\n\n.footer__content span {\n  font-weight: 700;\n}\n"},"lineCount":321}},"error":null,"hash":"e5307f3bbf995023d7c3687541964edf","cacheData":{"env":{}}}