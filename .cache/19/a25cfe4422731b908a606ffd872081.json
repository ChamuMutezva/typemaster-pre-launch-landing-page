3px;\r\n}\r\n\r\nnav {\r\n    padding: 0 24px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.hero__content {\r\n    padding: 0 24px;\r\n    margin-top: 60px;\r\n}\r\n\r\n.main__btn {\r\n    @include mainBtn;\r\n}\r\n\r\n.hero__content__title {\r\n    font-size: calculateRem(48);\r\n    line-height: 48px;\r\n    font-weight: 900;\r\n}\r\n\r\n.hero__content__description {\r\n   @include contentText;\r\n}\r\n\r\n.hero__content__primary {\r\n    margin-top: 32px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    font-size: calculateRem(16);\r\n}\r\n\r\n.hero__content__btn {\r\n    background-color: $orange-bg;\r\n    color: $white-tx;\r\n}\r\n\r\n.hero__content__span {\r\n    display: inline-block;\r\n    font-weight: 600;\r\n    color: $para-tx-color;\r\n}\r\n\r\n.hero__img__img {\r\n    width: 100%;\r\n    margin-left: 24px;\r\n    margin-top: 67px;\r\n    border-radius: 24px 0 0 24px;\r\n}\r\n\r\n","styles\\abstract\\_mixins.scss":"// Function for converting a px based font-size to rem.\r\n@function calculateRem($size) {\r\n    $remSize: $size / 16;\r\n    //Default font size on html element is 100%, equivalent to 16px;\r\n    @return #{$remSize}rem;\r\n}\r\n@mixin mainBtn {\r\n\tfont-size: calculateRem(16);\r\n\tpadding: .6rem 1.1rem;\r\n\tfont-weight: 600;\r\n\tline-height: 26px;\r\n\tbackground-color: #e8eff2;\r\n\tborder: 1px solid transparent;\r\n\tborder-radius: 8px;\r\n}\r\n@mixin contentText {\r\n    font-size: calculateRem(16);\r\n    line-height: 26px;\r\n    color: $para-tx-color;\r\n    margin-top: 35px;\r\n}","styles\\layouts\\_primary.scss":"//PRIMARY SECTION*******\\\\\r\n.primary {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    margin-top: 15px;\r\n    gap: 70px 20px;\r\n}\r\n\r\n.primary__img1__img,\r\n.primary__img2__img {\r\n    width: 100%;\r\n    height: 100%;\r\n    border-radius: 20px;\r\n}\r\n\r\n.primary__img1__img {\r\n    opacity: 0.2;\r\n\r\n}\r\n\r\n.primary__img1 {\r\n    background-color: $orange-bg;\r\n    border-radius: 0 20px 20px 0;\r\n}\r\n\r\n.primary__img2 {\r\n    grid-area: 1/2/1/4;\r\n    border-radius: 20px;\r\n    margin-right: 24px;\r\n}\r\n\r\n.primary__content {\r\n    grid-area: 2/1/2/4;\r\n    margin: 0 24px;\r\n}\r\n\r\n.primary__content__title {\r\n    font-size: calculateRem(32);\r\n    padding: 0px 36px;\r\n    line-height: 36px;\r\n    font-weight: 900;\r\n    text-align: center;\r\n    color: #162542;\r\n}\r\n\r\n.primary__content__description {\r\n    @include contentText;\r\n    margin-top: 25px;\r\n    text-align: center;\r\n}\r\n","styles\\layouts\\_secondary.scss":"//-------------cards section ---------//\r\n\r\n.secondary {\r\n    margin-top: 117px;\r\n\r\n    .card {\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        justify-content: center;\r\n        gap: 45px;\r\n        margin-bottom: 45px;\r\n    }\r\n\r\n    .card__image__holder {\r\n        background-color: $orange-bg;\r\n        width: 65px;\r\n        height: 65px;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        border-radius: 20px;\r\n    }\r\n\r\n    .card__title {\r\n        font-size: calculateRem(24);\r\n        line-height: 28px;\r\n        font-weight: 900;\r\n        color: #162542;\r\n        margin: 0 100px;\r\n        text-align: center;\r\n    }\r\n\r\n    .card__description {\r\n        @include contentText;\r\n        margin-top: 0;\r\n        padding: 0 24px;\r\n        text-align: center;\r\n    }\r\n}","styles\\layouts\\_footer.scss":".footer__content {\r\n    @include contentText;\r\n\ttext-align: center;\r\n    padding-bottom: 24px;\r\n    span {\r\n        font-weight: 700;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"3ffdd6ac06dbf52b0385aa1bba9d8020","cacheData":{"env":{}}}